<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="OptsLib.Examples.AvaloniaOptsEditor.App">
    <Application.Styles>
        <FluentTheme Mode="Dark" />
        
    </Application.Styles>
    <Application.Resources>
        <Color x:Key="SystemAccentColor">#371F92</Color>
        <Color x:Key="SystemAccentColorDark1">#29176C</Color>
        <Color x:Key="SystemAccentColorDark2">#1B0F46</Color>
        <Color x:Key="SystemAccentColorDark3">#060413</Color>
        <Color x:Key="SystemAccentColorLight1">#4527B8</Color>
        <Color x:Key="SystemAccentColorLight2">#8051FF</Color>
        <Color x:Key="SystemAccentColorLight3">#AC8DFF</Color>
    </Application.Resources>
</Application>
